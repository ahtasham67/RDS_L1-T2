        <Button fx:id="searchButtonName" layoutX="592.0" layoutY="73.0" mnemonicParsing="false" onAction="#FOODsearchNameAndRestaurant" prefHeight="47.0" prefWidth="164.0" style="-fx-background-color: #05445E;" text="Search" textFill="WHITE">
